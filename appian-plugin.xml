<appian-plugin name="Document Automation" key="com.lowcodeminds.plugins.documentutilities">
	<plugin-info>
		<description> This smart service will automatically replace the all field tags in a word document based on JSON tags Structure and converts the document into docx format.</description>
		<vendor name="LowCode Minds" url="https://www.lowcodeminds.com/" />
		<version>5.0.3</version>
		<application-version min="23.2" />
	</plugin-info>
    <smart-service name="Document Template Automation" key="dta" class="com.lowcodeminds.plugins.documentutilities.ReadAllFields" /> 
	<smart-service name="Document Template Automation V2" key="TagsReplacementDocAutomateV2" class="com.lowcodeminds.plugins.documentutilities.TemplateSmartService" />

</appian-plugin>